          <tr>
            <td>Social Security Number (###-##-####) <span class="style1"> * </span></td>
            <td>Date of Birth (YYYY-MM-DD) <span class="style1"> * </span></td>
            <td>&nbsp;</td>
          </tr>
          <tr>
            <td width="40%"><input name="ssn" type="text" class="input_small22" value="Enter SSN" /></td>
            <td width="60%"><input name="dob" type="text" class="input_small22" value="Enter DOB" /></td>
            <td width="60%"><img src="images/btn_submit_form.png" alt="create link" width="135" height="40" onclick="create_client.submit();"/></td>
          </tr>
<tr></tr>
        </table>
          <span class="style1"></span></td>
            </tr>
        </table></td>
        </tr>
      <tr>
        <td colspan="3" align="left" class="font_white_12">&nbsp;</td>
